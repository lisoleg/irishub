sync function (): Promise<void> {\n      if (this._depsLoaded) {\n        if (this.newAccount.nextAvailable) {\n          await this.$store.dispatch('common/wallet/addAccount')\n        } else {\n          await this.$store.dispatch('common/wallet/addAccount', this.newAccount.pathIncrement)\n        }\n      }\n      this.reset()\n    },\n  },\n})\n"],"sourceRoot":""}]}